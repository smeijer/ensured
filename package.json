{
	"name": "ensured",
	"version": "0.0.4",
	"description": "Tiny runtime assertions that preserve error messages in production.",
	"license": "MIT",
	"repository": "smeijer/ensured",
	"funding": "https://github.com/smeijer/ensured?sponsor=1",
	"author": "Stephan Meijer <stephan.meijer@gmail.com> (meijer.ws)",
	"bugs": "https://github.com/smeijer/ensured/issues",
	"homepage": "https://github.com/smeijer/ensured#readme",
	"type": "module",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./dist/esm/index.d.ts",
				"default": "./dist/esm/index.js"
			},
			"require": {
				"types": "./dist/commonjs/index.d.ts",
				"default": "./dist/commonjs/index.js"
			}
		}
	},
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"test": "prettier -w . && eslint . --fix && jest",
		"build": "tshy",
		"prepare": "npm run build"
	},
	"files": [
		"dist",
		"license"
	],
	"keywords": [
		"node",
		"module",
		"boilerplate"
	],
	"devDependencies": {
		"@smeijer/eslint-config": "^1.0.2",
		"@smeijer/tsconfig": "^1.0.0",
		"tshy": "^3.0.3",
		"typescript": "^5.9.3"
	},
	"tshy": {
		"exports": {
			"./package.json": "./package.json",
			".": "./src/index.ts"
		}
	},
	"main": "./dist/commonjs/index.js",
	"types": "./dist/commonjs/index.d.ts",
	"module": "./dist/esm/index.js"
}
